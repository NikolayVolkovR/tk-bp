#mochs {
  width: 100%;
  overflow: hidden;
}
#mocha-report {
  h1 {
    font-size: 18px;
    margin: 0 0 5px 0;
    padding: 0;
  }

  h2 {
    font-size: 14px;
    margin: 0 0 5px 0;
    padding: 0;
  }

  span.duration {
    margin-left: 20px;
  }

  ul {
    @media only screen and (max-width: 930px) {
      margin: 0 0 0 15px;
      padding: 0;
    }
    @media only screen and (max-width: 560px) {
      margin: 0 0 0 5px;
      padding: 0;
    }
  }
}
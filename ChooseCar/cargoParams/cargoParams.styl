.chooseCar__cargoParams {
  flex-basis: 29%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;

  @media only screen and (max-width: 930px) {
    flex-basis: 46%;
    margin-top: 20px;
  }
  @media only screen and (max-width: 560px) {
    order: 1;
    flex-basis: 100%;
  }

  .chooseCar__cargoParams-header {
    flex-basis: 100%;
    font-size: 20px;
    font-weight: bold;
    text-align: center;
    margin-bottom: 10px;

    @media only screen and (max-width: 930px) {
      text-align: left;
    }
  }

}
.about {
  display: flex;

  @media only screen and (max-width: 500px) {
    flex-direction: column;
  }
  .left-col {
    flex-basis: 300px;
    flex-shrink: 0;

    @media only screen and (max-width: 670px) {
      flex-basis: 40%;
    }
    @media only screen and (max-width: 500px) {
      flex-basis: 100%;
    }
  }
  .right-col {
    margin-left: 20px;

    @media only screen and (max-width: 500px) {
      margin: 20px 0 0 0;
    }

    .big-screen {
      @media only screen and (max-width: 750px) {
        display: none
      }
    }
    p {
      font-size: 16px;
      line-height: 1.7;
      text-align: justify;

      &.last {
        margin-top: 20px;
      }
    }
  }
}

.tarifs {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 30px;

  img.question {
    margin: -9px 0 0 3px;
    width: 20px;
    cursor: pointer;
  }
  .tarif {
    display: flex;
    flex-basis: 48%;
    justify-content: space-between;
    margin-bottom: 70px;

    @media only screen and (max-width: 740px) {
      flex-basis: 100%;
    }

    .tarif__left-side {
      flex-basis: 30%;
      margin-right: 20px;
      display: flex;
      flex-direction: column;
      justify-content: space-between;

      .tarif__params-block {

        .tarif__image {
          img {
            width: 100%;
          }
        }
        .tarif__name {
          font-size: 18px;
          text-align: center;
          font-weight: bold;
          color: color_2;
        }
        .tarif__params-row {
          display: flex;
          justify-content: space-between;

          .tarif__gruz {
            flex-basis: 50%;
            text-align: center;
            span {
              font-weight: bold;
              font-size: 18px;
              color: color_4;
            }
          }
          .tarif__volume {
            flex-basis: 50%;
            text-align: center;
            span {
              font-weight: bold;
              font-size: 18px;
              color: color_4;
            }
          }
        }
      }
      .tarif__order-button {
        margin-top: 15px;
        padding: 10px 0;
        text-transform: uppercase;
        font-size: 16px;
        background: color_green_3;
        color: #fff;
        text-align: center;
        cursor: pointer;
        border-radius: 5px;

        @media only screen and (max-width: 380px) {
          font-size: 14px;
        }
      }
    }
    .tarif__right-side {
      flex-basis: 70%;
      display: flex;
      flex-direction: column;
      justify-content: space-between;

      .tarif__prices-header {
        display: flex;

        .tarif__price-header-col1 {
          flex-basis: 30%;
          border-bottom: solid 1px color_3;
        }
        .tarif__price-header-col2, .tarif__price-header-col3 {
          flex-basis: 35%;
          padding: 5px;
          background: color_green_1;
          text-align: center;
          border-bottom: solid 1px color_3;
        }
        .tarif__price-header-col3 {
          background: color_green_2;
        }
      }
      .tarif__price-line {
        display: flex;

        .tarif__price-line-col1 {
          flex-basis: 30%;
          border-bottom: solid 1px color_3;
          padding: 5px;
        }
        .tarif__price-line-col2, .tarif__price-line-col3 {
          padding-top: 5px;
          display: flex;
          flex-basis: 35%;
          border-bottom: solid 1px color_3;
          justify-content: center;
          align-items: center;
        }
      }
      .tarif__fast-order {
        margin-top: 20px;

        .tarif__fast-order-header {
          font-weight: bold;
          border-bottom: solid 1px color_1;
          padding-bottom: 3px;
        }
        .tarif__fast-order-content {
          display: flex;
          border-bottom: solid 1px color_1;
          padding: 5px 0;

          .tarif__fast-order-left {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            flex-basis: 50%;
          }
          .tarif__fast-order-right {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            flex-basis: 50%;

            div {
              text-align: center;
            }
          }
        }
        /*.tarif__fast-order-line {
          display: flex;
          justify-content: space-between;
          padding-top: 10px;

          &:last-child {
            //margin-top: 10px;
            border-bottom: solid 1px color_1;
            padding-bottom: 10px;
          }
          .triple {
            flex-basis: 33%;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 18px;

            &:first-child {
              justify-content: flex-end;
            }
            &:last-child {
              justify-content: flex-start;
            }
          }
          .double {
            flex-basis: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
          }
        }*/
      }
    }
  }
}
.page-snippet {
  display: flex;
  flex-wrap: wrap;
  margin: 30px 0;

  @media only screen and (max-width: 650px) {
    flex-direction: column;
    align-items: center;
    blockquote, figure {
      flex-basis: 100%;
    }
  }

  blockquote {
    display: flex;
    align-items: center;
    flex-basis: 50%;
    margin-bottom: 0!important;

    .special-offer {
      background: #eee;
      border-radius: 5px;

      header {
        display: block;
        border-radius: 5px;
        background: color_1;
        color: #fff;
        padding: 10px 15px;
        margin-bottom: 5px;
      }
      p {
        padding: 10px;
        line-height: 1.6;
      }

    }
  }
  figure {
    flex-basis: 50%;

    img {
      @media only screen and (max-width: 650px) {
        width: 90%;
      }
      //width: 400px;
      border-radius: 5px;
    }
    figcaption {
      margin-top: 10px;
    }
  }
}
.order-types {
  margin: 30px 0;

  >header {
    flex-basis: 100%;
    //margin-bottom: 30px;
    font-size: 24px;
    text-align: center;
  }

  .wraper {
    display: flex;
    justify-content : center; //space-around;
    flex-wrap: wrap;
    text-align: center;

    .order-option {
      position: relative;
      flex-basis: 30%;
      padding: 2%;
      padding-bottom: 145px;
      background: #eee;
      font-size: 16px;
      margin-top: 30px;
      margin-right: 3%;
      border-radius: 5px;

      &:last-child {
        //margin-right: 0;
      }

      &.order-option-special {
        background: color_neighbors;

        .order-block {
          button {
            background: #5F905F;
          }
        }
      }

      @media only screen and (max-width: 765px){
        flex-basis: 48%;
        margin-bottom: 20px;
      }
      @media only screen and (max-width: 450px){
        flex-basis: 100%;
      }

      header {
        text-align: center;
        font-size: 18px;
        font-weight: bold;
        margin-bottom: 15px;
      }

      .details {
        text-decoration: underline;
      }
      .free-feed {
        font-size: 16px;
        text-align: center;
        margin-bottom: 10px;
        font-weight: bold;
      }
      .order-block {
        position: absolute;
        bottom: 4%;
        left: 0;
        right: 0;
        margin: 0 25%;
        width: 50%;


        .min-order {
          margin: 0 auto 10px;
          text-align: center;
        }
        button {
          background: color_green_3;
          padding: 10px;
          border: none;
          color: #fff;
          display: block;
          width: 100%;
          text-transform: uppercase;
          border-radius: 5px;
        }
      }

    }
  }
}

color_1_2 = #A6B4EB;
color_1_3 = #C8D1F5;

color_2 = #4d4d4d;
color_3 = #999999;
color_3_hover = #ccc;
color_4 = #808080;
color_4_hover = #bbb;
color_5 = #333;
color_5_hover = #777;

color_6 = #b3b3b3;
color_7 = #e6e6e6;


color_green_1 = #a6e2a6;
color_green_2 = #91c491;
color_green_3 = #5F905F;

color_neighbors = #D8FF9A;

html, body
  height: 100%;
  font-family: Century Gothic,CenturyGothic,AppleGothic,sans-serif;
  color: color_5;


h1 {
  font-size: 30px;
  color: color_2;

  &.main-page {
    margin: 50px 0 30px;
    text-align: center;
  }
}

h2 {
  font-size: 26px;
  color: color_2;

  &.main-page {
    margin: 50px 0 20px;
    text-align: center;
  }
}
h3 {
  font-size: 22px;
  color: color_2;
}

p {
  line-height: 1.8;
}
ul {
  margin: 0;
  padding: 0 0 0 30px;

  li {
    margin: 5px 0 0;
  }
}
blockquote {
  border-left-color: color_1_2;
}
main {
  margin-top: 30px;
}
.breadcrumb {
  background: transparent;
  padding: 0;
  color: color_4;
  font-size: 17px;

  > li + li:before {
    content: ">";
  }

  li {
    a {
      color: color_4;

      &:hover {
        color: color_4_hover;
        text-decoration: none;
      }
    }
    &.active {
      color: color_6;
    }
  }
}
.wrap {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-top: 20px;

  .in-wrap {
    padding: 0 15px;

    @media only screen and (min-width: 1000px) {
      padding: 0;
    }
    @media only screen and (max-width: 530px) {
      padding: 0 10px;
    }
  }
}
.m-container {
  margin: 0 auto;
  width: 100%;

  @media only screen and (min-width: 1000px) {
    width: 1000px;
  }

/*  @media only screen and (max-width: 576px) {
    width: 100%;
  }
  @media only screen and (min-width: 576px) and (max-width: 767px) {
    width: 540px;
  }
  @media only screen and (min-width: 768px) and (max-width: 991px) {
    width: 720px;
  }
  @media only screen and (min-width: 992px) and (max-width: 1199px) {
    width: 960px;
  }

  @media only screen and (min-width: 1200px) {
    width: 1140px;
  }
  */
}

.header {
  display: flex;
  justify-content: space-between;

  @media only screen and (max-width: 530px) {
    flex-direction: column;
  }

  .header__logo {
    flex-basis: 380px;
    @media only screen and (max-width: 720px) {
      flex-basis: 320px;
    }
    @media only screen and (max-width: 610px) {
      flex-basis: 280px;
    }
    @media only screen and (max-width: 530px) {
      flex-basis: 100%;
    }
  }

  .header__contacts {
    flex-basis: 50%;
    margin-top: 9px;

    @media only screen and (max-width: 870px) {
      flex-basis: 40%;
    }
    @media only screen and (max-width: 610px) {
      flex-basis: 42%;
    }
    @media only screen and (max-width: 530px) {
      margin-top: 0;
    }

    .top {
      display: flex;
      justify-content: space-between;
      align-items: center;
      border-bottom: solid 1px color_1;

      .social {
        margin-left: 10px;

        img {
          width: 32px;

          @media only screen and (max-width: 870px) {
            display: none;
          }
        }
      }
      .phone {
        font-size: 36px;
        color: color_2;

        @media only screen and (max-width: 720px) {
          font-size: 30px;
        }
        @media only screen and (max-width: 610px) {
          font-size: 26px;
        }
        @media only screen and (max-width: 530px) {
          font-size: 36px;
        }
        .pref {
          color: color_3;
          font-size:24px;
          @media only screen and (max-width: 720px) {
            font-size: 18px;
          }
          @media only screen and (max-width: 610px) {
            font-size: 16px;
          }
          @media only screen and (max-width: 530px) {
            font-size: 24px;
          }
        }
      }
    }
    .bottom {
      display: flex;
      justify-content: flex-end;
      margin-top: 3px;
      a {
        color: color_4;
        font-size: 15px;
        padding-left: 10px;

        &:first-child {
          padding-right:10px;
        }
        &:hover {
          color: color_4_hover;
        }
      }
    }
    .social-small {
      display: none;
      justify-content: flex-end;
      margin-top: 7px;

      @media only screen and (max-width: 870px) {
        display: flex;
      }
      @media only screen and (max-width: 530px) {
        justify-content: flex-start;
        margin-top: -15px;
      }
      @media only screen and (max-width: 350px) {
        justify-content: flex-start;
        margin-top: -15px;
        width: 30%;
      }
      img {
        width: 28px;
        margin: 0 2px;

        @media only screen and (max-width: 530px) {
          width: 32px;
        }
        @media only screen and (max-width: 350px) {
          width: 24px;
        }
      }
    }
  }
}
.menu {
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin: 10px auto 0;
  padding: 0 0 7px;
  border-bottom: solid 1px color_6;

  @media only screen and (min-width: 1000px) {
    width: 1000px;
  }
  @media only screen and (max-width: 870px) {
    flex-wrap: wrap;
    padding: 0 15%;
    padding-bottom: 0;
    margin-top: 0;
  }
  @media only screen and (max-width: 610px) {
    flex-wrap: wrap;
    padding: 0 10%;
    margin-top: 0;
  }
  @media only screen and (max-width: 530px) {
    flex-wrap: wrap;
    padding: 0;
    margin-top: 10px;
  }
  li {
    flex-basis: 130px;
    list-style-type: none;
    text-align: center;

    &.make-order {
      color: color_5;
      font-size: 16px;
      cursor: pointer;
    }

    @media only screen and (max-width: 870px) {
      flex-basis: 33%;
      padding-top: 17px;
      padding-bottom: 7px;
    }
    a {
      color: color_5;
      font-size: 16px;

      @media only screen and (max-width: 530px) {
        font-size: 14px;
      }
      &:hover {
        color: color_5_hover;
        text-decoration: none;
      }
    }

    img {
      display: block;
      margin: 0 auto;
      height: 55px;
      margin-bottom: 7px;
    }

    &:first-child {
      flex-basis: 390px;
      @media only screen and (max-width: 870px) {
        flex-basis: 33%;
      }
    }
    &:last-child {
      flex-basis: 70px;
      margin-left: 20px;
      @media only screen and (max-width: 870px) {
        flex-basis: 33%;
        margin-left: 0;
      }
    }
  }

}
.content {
  margin: 0 auto;
  border-top: solid 3px color_7;

  @media only screen and (min-width: 1000px) {
    width: 1000px;
  }
}
.footer {
  background: color_1;
  color: #fff;
  padding: 20px 15px;
  margin-top: 40px;

  @media only screen and (min-width: 1000px) {
    padding: 20px 0;
  }

  .m-container {
    display: flex;
    justify-content: space-between;

    @media only screen and (max-width: 530px) {
      flex-direction: column;
    }

    .footer__logo {
      @media only screen and (max-width: 530px) {
        margin: 0 auto;
      }

      .top {
        border-bottom: solid 2px #e08716;
        font-size: 29px;
        text-transform: uppercase;
        text-align: center;
        letter-spacing: 1px;
        padding-bottom: 3px;

        @media only screen and (max-width: 720px) {
          font-size: 26px;
        }
        @media only screen and (max-width: 610px) {
          font-size: 22px;
        }
      }
      .bottom {
        font-size: 17px;
        text-transform: uppercase;
        text-align: center;
        letter-spacing: 1px;
        margin-top: 1px;

        @media only screen and (max-width: 720px) {
          font-size: 15px;
        }
        @media only screen and (max-width: 610px) {
          font-size: 13px;
        }
      }
    }
    .footer__social {
      display: block;

      @media only screen and (max-width: 720px) {
        display: none;
      }

      a {
        padding: 0 1px;

        img {
          width: 32px;
        }
      }
    }

    .footer__contacts {

      @media only screen and (max-width: 530px) {
        margin-top: 10px;
        text-align: center;
      }

      .phone {
        font-size: 32px;

        @media only screen and (max-width: 720px) {
          font-size: 30px;
        }
        @media only screen and (max-width: 610px) {
          font-size: 26px;
        }
        .prefix {
          @media only screen and (max-width: 720px) {
            font-size: 18px;
          }
          @media only screen and (max-width: 610px) {
            font-size: 16px;
          }
          font-size: 22px;
        }
      }
      .footer__social_small {
        display: none;
        justify-content: flex-end;
        margin-top: 5px;

        @media only screen and (max-width: 720px) {
          display: flex;
        }
        @media only screen and (max-width: 530px) {
          justify-content: center;
          margin-top: 10px;
        }
        a {
          padding: 0 1px;

          img {
            @media only screen and (max-width: 720px) {
              width: 28px;
            }
            width: 32px;
          }
        }
      }
    }
  }
}

.image-hint {
  margin-top: 10px;
  font-style: italic;
  color: color_2;
}


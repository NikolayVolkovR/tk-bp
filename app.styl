.order-types-compact__line {
  display: flex;
  flex-wrap: wrap;
  border-radius: 5px;
  border: solid 1px color_3;
  margin-top: 20px;

  .order-types-compact__item {
    flex-basis: calc(25% - 20px);
    margin: 10px;
    background: #eee;
    border-radius: 5px;
    padding: 10px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;

